{
  "name": "react_repo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm install && vite",
    "build": "npm install && rmdir /s /q node_modules\\.vite-temp 2>nul || exit 0 && npx tsc -b && npx vite build",
    "build:prod": "npm install && rmdir /s /q node_modules\\.vite-temp 2>nul || exit 0 && npx tsc -b && set BUILD_MODE=prod && npx vite build",
    "lint": "npm install && npx eslint .",
    "preview": "npm install && npx vite preview",
    "install-deps": "npm install",
    "clean": "rmdir /s /q node_modules 2>nul || exit 0 && del /f /q package-lock.json 2>nul || exit 0 && echo Clean complete.",
    "update-readers": "node scripts/update-readers.mjs",
    "cap": "npx cap",
    "cap:add": "npx cap add",
    "cap:copy": "npx cap copy",
    "cap:open": "npx cap open",
    "cap:sync": "npx cap sync",
    "cap:update": "npx cap update",
    "build-android": "npm run build && npx cap copy android && npx cap open android",
    "build-ios": "npm run build && npx cap copy ios && npx cap open ios",
    "sync": "npx cap sync",
    "update-android": "npx cap update android",
    "update-ios": "npx cap update ios"
  },
  "dependencies": {
